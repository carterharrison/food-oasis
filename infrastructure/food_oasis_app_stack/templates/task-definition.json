[
  {
    "name": "foodoasis-container",
    "image": "foodoasisla/foodoasisla::${image_tag}",
    "cpu": ${container_cpu},
    "memory": ${container_memory},
    "portMappings": [
      {
        "containerPort": 5000
      }
    ],
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group" : "/${cluster_name}/${task_name}",
        "awslogs-region": "${region}",
        "awslogs-stream-prefix": "ecs",
        "awslogs-create-group": "true"
      }
    },
    "healthCheck": {
      "command": [
        "CMD-SHELL",
        "node -e 'console.log(200);' || exit 1"
      ],
      "interval": 20,
      "timeout": 30,
      "retries": 2
    }
  
  }
]
