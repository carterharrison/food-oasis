[
  {
    "name": "foodoasis-container",
    "image": "470363915259.dkr.ecr.us-east-2.amazonaws.com/food-oasis",
    "cpu": ${container_cpu},
    "memory": ${container_memory},
    "portMappings": [
      { "containerPort": ${container_port}, "hostPort": ${container_port} },
      { "containerPort": 80, "hostPort": 80 }
    ],
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group" : "/${cluster_name}/${task_name}",
        "awslogs-region": "${region}",
        "awslogs-stream-prefix": "ecs",
        "awslogs-create-group": "true"
      }
    },
    "healthCheck": {
      "command": [
        "CMD-SHELL",
        "node -e 'console.log(200);' || exit 1"
      ],
      "interval": 20,
      "timeout": 30,
      "retries": 2
    }
  
  }
]
